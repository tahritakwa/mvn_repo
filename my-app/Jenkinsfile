pipeline {
  environment {
    imagename = "mvn_image"
    dockerImage = ''
    contanername= "mnv_cont"
  }
  agent any
  stages {
    stage('Cloning Git') {
      steps {
        git([url: 'https://github.com/tahritakwa/mvn_repo.git', 
        branch: 'master',
        credentialsId: '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'])

      }
    }
    stage('Building image') {
      steps{
        script {
          dockerImage = docker.build -t imagename .
        }
      }

    }
