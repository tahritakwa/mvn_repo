pipeline {
  agent none
  stages {
    stage('Maven Install') {
      agent {
        docker {
          image 'maven:3.8.4'
        }
      }
      steps {
        sh 'mvn clean install'
      }
    }
   stage('Docker Build') {
     agent any
       steps {
         sh 'docker build -t mvn_image .'
      }
    }
   stage('Docker Push') {
      agent any
      steps {
        sh 'echo ed146a65-c54e-4850-b69e-2a2419b123eb | docker login -u takwa99 --password-stdin'
        sh 'docker push mvn_image'
        }
      }
    }
  }
}
